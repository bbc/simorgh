{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "classic"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    "@babel/plugin-proposal-export-default-from",
    [
      "transform-rename-import",
      { "original": "@loadable/component", "replacement": "next/dynamic" }
    ]
  ]
}
