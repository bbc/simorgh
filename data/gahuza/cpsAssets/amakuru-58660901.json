            },
            {
              "text": "<itemMeta>gahuza/amakuru-58501820</itemMeta>",
              "meta": [
                {
                  "headlines": {
                    "shortHeadline": "Rwanda: Umukuru w'umugambwe RPD arapfunzwe",
                    "headline": "Rwanda: Umukuru w'umugambwe RPD Christopher Kayumba yahagaritswe",
                    "overtyped": "Rwanda: Umukuru w'umugambwe RPD arapfunzwe. "
                  },
                  "locators": {
                    "href": "https://www.bbc.com/gahuza/amakuru-58501820"
                  },
                  "summary": "Itangazo rya RIB (Rwanda Investigation Bureau) ku rubuga rwa Twitter rivuga ko ryapfunze Dr Christopher rimwagiriza ivyaha vyo gukoresha imibonano mpuzabitsina abantu batandukanye ku gahato.",
                  "timestamp": 1631195700000,
                  "language": "rw",
                  "passport": {
                    "category": {
                      "categoryId": "",
